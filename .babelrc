{
  "presets": [
    [
      "env", {
        "modules": false,
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }
    ],
    "stage-3", 
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    [
      "transform-react-jsx", 
      {
        "pragma": "h"
      }
    ],
    "lodash",
    "date-fns"
  ]
}
